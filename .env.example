# Настройки удаленного MySQL сервера
DBSYNC_REMOTE_HOST=your-remote-server.com
DBSYNC_REMOTE_PORT=3306
DBSYNC_REMOTE_USER=username
DBSYNC_REMOTE_PASSWORD=password

# Настройки локального MySQL сервера
DBSYNC_LOCAL_HOST=localhost
DBSYNC_LOCAL_PORT=3306
DBSYNC_LOCAL_USER=root
DBSYNC_LOCAL_PASSWORD=password

# Настройки дампа
DBSYNC_DUMP_TIMEOUT=300s
DBSYNC_DUMP_TEMP_DIR=./tmp
DBSYNC_DUMP_MYSQLDUMP_PATH=mysqldump
DBSYNC_DUMP_MYSQL_PATH=mysql

# Настройки CLI
DBSYNC_CLI_DEFAULT_CHARSET=utf8mb4
DBSYNC_CLI_INTERACTIVE_MODE=true
DBSYNC_CLI_CONFIRM_DESTRUCTIVE=true

# Настройки логирования
DBSYNC_LOG_LEVEL=info
DBSYNC_LOG_FORMAT=text
